# Auto detect text files and perform LF normalization
* text=auto
*.unity filter=lfs diff=lfs merge=lfs -text
*.contrtoller.meta filter=lfs diff=lfs merge=lfs -text
*.controller filter=lfs diff=lfs merge=lfs -text
*.mp3.meta filter=lfs diff=lfs merge=lfs -text
.mp3 filter=lfs diff=lfs merge=lfs -text
.wav.meta filter=lfs diff=lfs merge=lfs -text
.wav filter=lfs diff=lfs merge=lfs -text
*.ogg.meta filter=lfs diff=lfs merge=lfs -text
*.ogg filter=lfs diff=lfs merge=lfs -text
.asset.meta filter=lfs diff=lfs merge=lfs -text
*.asset filter=lfs diff=lfs merge=lfs -text
*.cs filter=lfs diff=lfs merge=lfs -text
.cs filter=lfs diff=lfs merge=lfs -text
.cs.meta filter=lfs diff=lfs merge=lfs -text
.asset filter=lfs diff=lfs merge=lfs -text
.ttf filter=lfs diff=lfs merge=lfs -text
.txt filter=lfs diff=lfs merge=lfs -text
.shadergraph filter=lfs diff=lfs merge=lfs -text
.shader filter=lfs diff=lfs merge=lfs -text
.png filter=lfs diff=lfs merge=lfs -text
.tga filter=lfs diff=lfs merge=lfs -text
.fbx filter=lfs diff=lfs merge=lfs -text
.prefab filter=lfs diff=lfs merge=lfs -text
.dll filter=lfs diff=lfs merge=lfs -text
.chm filter=lfs diff=lfs merge=lfs -text
.pdf filter=lfs diff=lfs merge=lfs -text
.preset filter=lfs diff=lfs merge=lfs -text
.shadersubgraph filter=lfs diff=lfs merge=lfs -text
.zip filter=lfs diff=lfs merge=lfs -text
.mask filter=lfs diff=lfs merge=lfs -text
.mat filter=lfs diff=lfs merge=lfs -text
.LFS filter=lfs diff=lfs merge=lfs -text
.obj filter=lfs diff=lfs merge=lfs -text
.exr filter=lfs diff=lfs merge=lfs -text
.vfx filter=lfs diff=lfs merge=lfs -text
